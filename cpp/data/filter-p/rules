filter (materialize)
//
filter 1 2 3
:: 3 4 5

@ 6 2 4

filter []
// filter p [] -> []
filter 1 2 3
[] 3

id 1 3

filter :: T
// (p x = T) |- filter p (x :: xs) -> x :: filter p xs
filter 1 2 3
:: 3 4 5
T 6
p 6 4

:: 1 4 7
filter 7 2 5

filter :: F
// (p x = F) |- filter p (x :: xs) -> filter p xs
filter 1 2 3
:: 3 4 5
F 6
p 6 4

filter 1 2 5