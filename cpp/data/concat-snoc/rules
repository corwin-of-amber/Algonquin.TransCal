:+ []
// [] :+ x -> x :: []
:+ 1 2 3
[] 2

:: 1 3 2

:+ ::
// (x :: xs) :+ y -> x :: (xs :+ y)
:+ 1 2 3
:: 2 4 5

:: 1 4 6
:+ 6 5 3

++ []
// [] ++ xs -> xs
++ 1 2 3
[] 2

id 1 3

++ ::
// (x :: xs) ++ ys -> x :: (xs ++ ys)
++ 1 2 3
:: 2 4 5

:: 1 4 6
++ 6 5 3

++ []r
// xs ++ [] -> xs
++ 1 2 3
[] 3

id 1 2
